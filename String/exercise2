public class Main {
    public static void main(String[] arg) {
        String str = "What are you doing today?";
        String split[] = str.split(" ", 0);
        int count=0;
        for (String s: split){
            if(s.contains("a")){
              count++;
            }
        }

            //System.out.println(s);
            System.out.println(count);
    }
}
